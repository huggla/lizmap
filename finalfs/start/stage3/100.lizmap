# Set in stage2:
# ---------------------------------------------------------
# set -ef +am
# isFirstRun
# VAR_*
# All functions in /start/functions

if [ "$isFirstRun" == "true" ]
then
   if [ ! -s "$VAR_CONFIG_FILE" ] && [ "$VAR_CONFIG_FILE" != "/etc/nginx/nginx.conf" ]
   then
      cp "/etc/nginx/nginx.conf" "$VAR_CONFIG_FILE"
   fi
fi
